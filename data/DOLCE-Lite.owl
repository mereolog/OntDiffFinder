    <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-endurant"/>
    <owl:inverseOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#host-of"/>
    <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#specifically-constantly-dependent-on"/>
    <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#feature"/>
